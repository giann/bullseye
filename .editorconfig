root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 100
tab_width = 4

[*.dart]
max_line_length = 120
indent_size = 2

[{*.bash,*.sh,*.zsh}]
indent_size = 2
tab_width = 2

[{*.har,*.json}]
indent_size = 2

[{*.yaml,*.yml,.analysis_options}]
indent_size = 2
